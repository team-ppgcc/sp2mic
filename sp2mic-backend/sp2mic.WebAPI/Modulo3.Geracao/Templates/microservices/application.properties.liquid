server.port=0
spring.application.name={{projectMetadataArtifactId}}
spring.profiles.active=zone1
spring.config.import=optional:configserver:http://{{consulHost}}:{{consulPort}}
spring.datasource.url=jdbc:sqlserver://{{databaseHost}}:{{databasePort}};database={{databaseName}};trustServerCertificate=true
spring.datasource.username={{databaseUserName}}
spring.datasource.password={{databasePassword}}
spring.datasource.driver-class-name=com.microsoft.sqlserver.jdbc.SQLServerDriver
spring.jpa.show-sql=true
spring.jpa.properties.hibernate.dialect=org.hibernate.dialect.SQLServerDialect
spring.jpa.properties.ddl-auto=none
management.endpoints.web.exposure.include=health,refresh
spring.h2.console.enabled=true
springdoc.swagger-ui.enabled=true
spring.cloud.consul.discovery.prefer-ip-address=true
